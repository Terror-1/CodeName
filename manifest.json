{
    "manifest_version": 3,
    "name": "codeName",
    "version": "1.0",
    "description": "Replace the GUC course codes with their corresponding course names",
    "permissions": ["activeTab","scripting"],
    "action": {
      "default_icon": {
        "16": "images/icon16-2.png",
        "48": "images/icon48-2.png",
        "128": "images/icon128-2.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  